&gpi_dma0 {
	status = "okay";
};

&gpi_dma1 {
	status = "okay";
};

&i2c1 {
	clock-frequency = <100000>;
	status = "okay";

	typec-mux@42 {
		compatible = "fcs,fsa4480";
		reg = <0x42>;
		interrupts-extended = <&tlmm 2 IRQ_TYPE_LEVEL_LOW>;
		vcc-supply = <&vreg_bob_3p296>;
		mode-switch;
		orientation-switch;
		svid = /bits/ 16 <0xff01>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;

				fsa4480_sbu_mux: endpoint {
					remote-endpoint = <&pmic_glink_sbu>;
				};
			};
		};
	};
};

