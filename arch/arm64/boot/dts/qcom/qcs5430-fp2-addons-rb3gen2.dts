/dts-v1/;

#include "qcs5430-fp1-addons-rb3gen2.dts"

/ {
	model = "Qualcomm Technologies, Inc. qcs5430 fp2 addons rb3gen2 platform";
	compatible = "qcom,qcs5430-fp2-addons-rb3gen2","qcom,qcs5430";

	cpus {
		cpu-map {
			cluster0 {
				core6 {
					cpu = <&CPU6>;
				};

				core7 {
					cpu = <&CPU7>;
				};
			};
		};
	};
};

&CPU6 {
	enable-method = "psci";
};

&CPU7 {
	enable-method = "psci";
};

&pcie1 {
        status = "ok";
};
