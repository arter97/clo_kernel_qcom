	       regulator-min-microvolt = <5000000>;
	       regulator-max-microvolt = <5000000>;
	       regulator-boot-on;
	       enable-active-high;
	};

	connector_usb2: connector_usb2 {
		compatible = "gpio-usb-b-connector";
		vbus-gpio = <&pm7325_gpios 9 GPIO_ACTIVE_HIGH>;
		id-gpio = <&tlmm 61 GPIO_ACTIVE_HIGH>;
		vbus-supply = <&vbus_supply_regulator>;

		pinctrl-names = "default";
		pinctrl-0 = <&usb2_vbus_det_default>,
			     <&usb2_id_det_default>;
		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				usb2_port_connector: endpoint {
					remote-endpoint = <&usb2_port>;
				};
			};
		};
	};
};

&usb_2 {
	reg = <0x0 0x08c00000 0x0 0x100000>;

	interrupts-extended = <&intc GIC_SPI 242 IRQ_TYPE_LEVEL_HIGH>,
				<&intc GIC_SPI 240 IRQ_TYPE_LEVEL_HIGH>,
				<&pdc 12 IRQ_TYPE_LEVEL_HIGH>,
